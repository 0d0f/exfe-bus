BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//exfe//exfe.com //
X-WR-CALNAME:{{ .Cross.Title }}
X-WR-CALDESC:exfe cal
X-WR-TIMEZONE:{{ .Timezone }}
BEGIN:VEVENT
UID:{{ .Cross.Id }}{{ .To_identity.Id }}@exfe
DTSTAMP:{{ if .Cross.Time.Begin_at.UTCTime }}{{ .Cross.Time.Begin_at.UTCTime "20060102T150405" }}{{ else }}Time to be decided{{ end }}
DESCRIPTION:{{ $s := limit .Cross.Description 233 }}{{ replace $s "\n" "\\n" }}
DTSTART:{{ .Cross.Time.Begin_at.UTCTime "20060102T150405" }}
LOCATION: {{ replace .Cross.Title "\n" "\\n" }} - {{ replace .Cross.Description "\n" "\\n" }}
SUMMARY:{{ .Cross.Title}}
URL:https:{{ .Cross.Link .Config.Site_url }}
BEGIN:VALARM
ACTION:DISPLAY
DESCRIPTION:exfe:{{ .Cross.Title }}
TRIGGER:-P1W
END:VALARM
END:VEVENT
END:VCALENDAR
